{
  "rules": {
    "linebreak-style": ["error", "unix"],
    "indent": ["error", 4],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "no-console": "off"
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jquery": true
  },
  "globals": {
    "timestrapConfig": true
  },
  "parser": "vue-eslint-parser",
  "extends": [
    "plugin:vue/recommended",
    "eslint:recommended"
  ],
  "plugins": [
    "vue"
  ],
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 6,
    "sourceType": "module"
  }
}
